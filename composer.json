{
    "name": "tightenco/nova-stripe",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "stripe/stripe-php": "^6.16"
    },
    "autoload": {
        "psr-4": {
            "Tightenco\\NovaStripe\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tightenco\\NovaStripe\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
